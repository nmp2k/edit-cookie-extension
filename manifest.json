{
  "manifest_version": 3,
  "name": "Toggle Extension",
  "version": "1.0",
  "description": "A simple extension with toggle button.",
  "host_permissions": [
  "https://*/*"
  ],
   "icons": {
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icons":{
    "48": "icon.png",
    "128": "icon.png"
  }
  },
  "permissions": [
    "activeTab",
    "cookies",
    "browsingData"
  ]
}
